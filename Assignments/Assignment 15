function percentageOfWorld1(calcPercentage){
  return (calcPercentage/7900)*100;
}
function describePopulation(country, population){
  const percentage = percentageOfWorld1(1441);
  const data = `${country} has ${population} million people, which is about ${percentage}% of the world`;
  return data;
}
console.log(describePopulation('China', 1441));
console.log(describePopulation('America', 5200));
console.log(describePopulation('India', 7500));

